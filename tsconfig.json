{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": false,
    "outDir": "app",
    "rootDir": "src"
  },
  "filesGlob": [
    "src/**/*.ts",
    "typings/main.d.ts"
  ],
  "files": [
    "src/api/authenticate.ts",
    "src/api/permissions.ts",
    "src/api/routes.ts",
    "src/api/users.ts",
    "src/dashboard/app.constants.ts",
    "src/dashboard/app.ts",
    "src/dashboard/components/codePanel.directive.ts",
    "src/dashboard/components/profileImage.directive.ts",
    "src/dashboard/components/userPanel.directive.ts",
    "src/dashboard/layout/debug/debug.ts",
    "src/dashboard/layout/home/home.ts",
    "src/dashboard/layout/login/login.ts",
    "src/dashboard/layout/root/root.ts",
    "src/dashboard/layout/users/users.ts",
    "src/dashboard/layout/users/usersList.ts",
    "src/dashboard/services/api.service.ts",
    "src/dashboard/services/auth.service.ts",
    "src/dashboard/services/profile.service.ts",
    "src/global.spec.ts",
    "src/server.ts",
    "typings/main.d.ts"
  ],
  "atom": {
    "rewriteTsconfig": true
  }
}
